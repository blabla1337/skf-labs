                    <tbody>
                <% @user.each do |user| %>     
                            <tr>
                                <td> <%= link_to user.username, edit_page_path(user) %></td>
                                <td> <%= user.title %></td>
                                <td> <%= user.is_admin %> </td>
                            </tr>
                    <% end %>
                    </tbody>
                    </table>
